add_to_a_million :: (){
	print("count to a million!");
	counter: int;
	counter = 0;

	start_time: int;
	start_time = time_nano_seconds();

	while(counter < 1000000){
		counter = counter + 1;
	}

	end_time: int;
	end_time = time_nano_seconds();

	print(end_time - start_time);
}

pow :: (number: int, to_power: int){
	counter:int;
	counter = 1;

	result: int;
	result = number;

	while(counter < to_power){
		result = result * number;
		counter = counter + 1;
	}

	<- result;
}

